def suma_numeros():
    n = int(input("¿Cuántos números deseas sumar?: "))
    total = 0
    for i in range(n):
        num = float(input(f"Ingrese el número {i+1}: "))
        total += num
    print("La suma es:", total)


def producto_numeros():
    n = int(input("¿Cuántos números deseas multiplicar?: "))
    producto = 1
    for i in range(n):
        num = float(input(f"Ingrese el número {i+1}: "))
        producto *= num
    print("El producto es:", producto)
def division():
    a = float(input("Ingrese el primer número: "))
    b = float(input("Ingrese el segundo número: "))
    if b == 0:
        print("Error: división entre cero no permitida.")
    else:
        print("Resultado:", a / b)
 
 
def factorial():
    n = int(input("Ingrese un número entero positivo: "))
    if n < 0:
        print("No existe factorial de números negativos.")
        return
 
    resultado = 1
    for i in range(1, n + 1):
        resultado *= i
 
    print(f"El factorial de {n} es: {resultado}")
def tabla_multiplicar():
    n = int(input("Ingrese el número de la tabla: "))
    print(f"Tabla del {n}")
    for i in range(1, 11):
        print(f"{n} x {i} = {n * i}")
 
 
def cuadrado_cubo():
    n = float(input("Ingrese un número: "))
    print("Cuadrado:", n ** 2)
    print("Cubo:", n ** 3)
